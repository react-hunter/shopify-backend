extends layout

block content
  h1 Content Commerce Inventory List
  p.lead Test for Shopify Admin API.
  hr
  .row
    each inventory in inventoryList
      .col-md-6
        h2 #{ inventory.id }
        p Inventory Quantity: #{ inventory.qty_on_hand }
        p Availalbe Date: #{ inventory.date_available }
        